{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "Nuestro primer proyecto en nodejs",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss ",
    "clean": "rimraf dist",
    "imagemin": "imagemin assets/page/* --out-dir dist/assets",
    "imageminCarousel": "imagemin assets/carousel/* --out-dir dist/assets/carousel",
    "imageminHotels": "imagemin assets/hotels/* --out-dir dist/assets/hotels",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin faq.html -d dist --htmlmin -o dist/faq.html && usemin form.html -d dist --htmlmin -o dist/form.html && usemin table.html -d dist --htmlmin -o dist/table.html",
    "build": "npm run clean && npm run scss && npm run imageminCarousel && npm run imageminHotels && npm run imagemin && npm run usemin",
    "postinstall": "ng build --aot --prod"
    },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
    "dependencies": {
    "express": "^4.17.1",
    "zone.js": "~0.8.26",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "~7.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://mccm2310@bitbucket.org/mccm2310/guia-hoteles.git"
  },
  "author": "Mirlet Campos",
  "license": "ISC",
  "homepage": "https://bitbucket.org/mccm2310/guia-hoteles#readme",
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "cssmin": "^0.4.3",
    "grunt": "^1.0.4",
    "grunt-browser-sync": "^2.2.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-imagemin": "^3.1.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.1",
    "gulp-sass": "^4.0.2",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.4.0",
    "node-sass": "^4.11.0",
    "onchange": "^5.2.0",
    "rimraf": "^2.6.3",
    "time-grunt": "^2.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "jquery": "^3.3.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.14.7"
  }
}
